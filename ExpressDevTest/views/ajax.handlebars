    수열 N <input type="text" id="msg" /> 번째
    <input type="button" id="ajax_test" value="ajax_test" />
    <div id="result"></div>
    <script>
      $('#ajax_test').click( function() {
	  var su = $("#msg").val(); 
	  if(isNaN(su) == true) {
			alert("숫자만 입력 하세요");
			return false;
	  } else {
	         if(su > 10){
			alert("10 이하 숫자만 입력");
			return false;
                   }
			
	  }
          $('#result').html('');
          $.ajax({
              url: 'http://13.115.40.134:3000/ajax',
              dataType: 'json',
              type: 'POST',
              data: {'msg':$('#msg').val()},
              success: function(result) {
                  if ( result['result'] == true ) {
                      $('#result').html(result['msg']);
                  }
              }
          });
      })
    </script>